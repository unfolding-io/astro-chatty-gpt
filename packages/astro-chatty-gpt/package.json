	"license": "MIT",
	"peerDependencies": {
		"astro": "^5.1.3"
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.53",
		"@astrojs/check": "^0.9.5",
		"@upstash/ratelimit": "^2.0.6",
		"@upstash/redis": "^1.35.6",
		"@upstash/search": "^0.1.5",
		"@wcj/html-to-markdown": "^2.1.1",
		"ai": "^5.0.76",
		"astro-integration-kit": "0.19.0",
		"html-to-markdown": "^1.0.0",
		"jsdom": "^24.0.0",
		"typescript": "^5.9.3",
		"vite-plugin": "^0.0.0"
