{
	"name": "@unfolding/astro-chatty-gpt",
	"version": "0.0.1",
	"description": "A chatbot for your astro site powered by Upstash search and gpt-5",
	"type": "module",
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
