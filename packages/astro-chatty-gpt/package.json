	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"prepublishOnly": "cdc build"
	},
	"keywords": [
		"astro-integration",
		"astro-component",
		"withastro",
		"astro",
		"tooling",
		"utils",
		"utility",
		"chatbot",
		"upstash",
		"openai",
		"gpt-5"
	],
	"author": "unfolding.io",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unfolding-io/astro-chatty-gpt.git"
